<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <meta name="google-adsense-account" content="ca-pub-8152532464536367">
    <meta name="referrer" content="no-referrer" />
    <!-- Open Graph metadata for social media sharing -->
    <meta
      property="og:title"
      content="Jailbreak Changelogs | Latest Updates & Patch Notes"
    />
    <meta
      property="og:description"
      content="Get updates on Jailbreak's latest features, bug fixes, and balance changes."
    />
    <meta
      property="og:image"
      content="https://res.cloudinary.com/dsvlphknq/image/upload/v1727536082/changelogs/changelog-image-344.png"
    />
    <meta property="og:image:width" content="2328" />
    <meta property="og:image:height" content="1054" />
    <meta property="og:url" content="https://jailbreakchangelogs.xyz" />
    <meta property="og:type" content="website" />

    <!-- Additional metadata for SEO -->
    <meta
      name="description"
      content="Jailbreak Changelogs: Your go-to source for the latest updates, patch notes, and feature releases in Roblox Jailbreak. Stay informed about new vehicles, weapons, map changes, and more!"
    />
    <meta name="robots" content="index, follow" />
    <meta
      name="keywords"
      content="Jailbreak, Roblox, changelogs, updates, patch notes, game features, Jailbreak updates"
    />

    <!-- Twitter Card metadata -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta
      name="twitter:title"
      content="Jailbreak Changelogs | Latest Updates & Patch Notes"
    />
    <meta
      name="twitter:description"
      content="Get up-to-date information on Jailbreak's latest features, bug fixes, and balance changes. Stay ahead in the game!"
    />
    <meta
      name="twitter:image"
      content="https://res.cloudinary.com/dsvlphknq/image/upload/v1727536082/changelogs/changelog-image-344.png"
    />

    <title><%= title %></title>

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css"
    />
    <link rel="stylesheet" href="styles/timeline.css" />
    <link rel="icon" type="image/svg+xml" href="/icon.svg" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Bangers&family=Luckiest+Guy&display=swap"
      rel="stylesheet"
    />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  </head>

  <body data-scroll-container>
    <%- include('partials/nav') %>
    <div id="loading-overlay" class="show">
      <div class="text-center">
        <img
          src="https://res.cloudinary.com/dsvlphknq/image/upload/v1727127650/jailbreak_logo.png"
          alt="Loading logo"
          class="loading-logo"
        />
      </div>
      <div class="d-flex justify-content-center mt-3">
        <div class="spinner-border custom-spinner" role="status">
          <span class="visually-hidden">Loading...</span>
        </div>
      </div>
    </div>

   
    <div
      class="offcanvas offcanvas-end"
      tabindex="-1"
      id="discordOffcanvas"
      aria-labelledby="discordOffcanvasLabel"
    >
      <div class="offcanvas-header">
        <h5 class="offcanvas-title" id="discordOffcanvasLabel">
          Join the Changelogs Discord
        </h5>
        <button
          type="button"
          class="btn-close text-reset"
          data-bs-dismiss="offcanvas"
          aria-label="Close"
        ></button>
      </div>
      <div class="offcanvas-body">
        <iframe
          src="https://discord.com/widget?id=1286064050135896064&theme=dark"
          width="100%"
          height="100%"
          allowtransparency="true"
          frameborder="0"
          sandbox="allow-popups allow-popups-to-escape-sandbox allow-same-origin allow-scripts"
        >
        </iframe>
      </div>
    </div>

    <div id="main-container" class="container-fluid">
      <div id="content-wrapper" class="row mt-3">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Home</a></li>
            <li class="breadcrumb-item active" aria-current="page">Timeline</li>
          </ol>
        </nav>
        <div class="col-12 mb-3">
          <div
            class="info-button"
            data-bs-toggle="tooltip"
            data-bs-placement="right"
            data-bs-custom-class="custom-tooltip"
            title="Looking for entries with images/videos/audio? Check out the Changelogs page in the navigation bar!"
          >
            <i class="bi bi-info-circle"></i>
            <span class="info-label">Info</span>
          </div>
        </div>

        <div class="col-12">
          <section id="content">
            <div id="timeline-container" class="container-fluid mt-5">
              <div id="timeline" class="timeline">
                <div class="timeline-entry-container left">
                  <div class="timeline-entry">
                    <!-- Content for the entry -->
                  </div>
                  <div class="timeline-line"></div>
                </div>
                <div class="timeline-entry-container right">
                  <div class="timeline-entry">
                    <!-- Content for the entry -->
                  </div>
                  <div class="timeline-line"></div>
                </div>
                <!-- Repeat as needed -->
              </div>
            </div>
          </section>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>
    
    <script src="scripts/main.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="scripts/timeline.js"></script>
    <button
      id="backToTop"
      class="btn rounded-circle position-fixed bottom-0 end-0 m-3"
    >
      <i class="bi bi-arrow-up-circle-fill"></i>
    </button>
    <div
      class="modal fade"
      id="dateFilterModal"
      tabindex="-1"
      aria-labelledby="dateFilterModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="dateFilterModalLabel">
              Select Date Range
            </h5>
            <button
              type="button"
              class="btn-close"
              data-bs-dismiss="modal"
              aria-label="Close"
            ></button>
          </div>
          <div class="modal-body">
            <div id="dateRangeFilter" class="row g-2">
              <div class="col-md-6">
                <label for="startDateBtn" class="form-label">Start Date:</label>
                <button
                  id="startDateBtn"
                  class="btn btn-outline-secondary w-100"
                >
                  <i class="bi bi-calendar-event me-2"></i
                  ><span>Select Start Date</span>
                </button>
                <input type="hidden" id="startDate" />
              </div>
              <div class="col-md-6">
                <label for="endDateBtn" class="form-label">End Date:</label>
                <button id="endDateBtn" class="btn btn-outline-secondary w-100">
                  <i class="bi bi-calendar-event me-2"></i
                  ><span>Select End Date</span>
                </button>
                <input type="hidden" id="endDate" />
              </div>
            </div>
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-secondary"
              data-bs-dismiss="modal"
            >
              Close
            </button>
            <button type="button" class="btn btn-primary" id="applyDateFilter">
              Apply Filter
            </button>
          </div>
        </div>
      </div>
    </div>
    <div class="toast-container position-fixed bottom-0 end-0 p-3">
      <div
        id="clearFilterToast"
        class="toast align-items-center text-white bg-success border-0"
        role="alert"
        aria-live="assertive"
        aria-atomic="true"
      >
        <div class="d-flex">
          <div class="toast-body">Filter cleared successfully.</div>
          <button
            type="button"
            class="btn-close btn-close-white me-2 m-auto"
            data-bs-dismiss="toast"
            aria-label="Close"
          ></button>
        </div>
      </div>
    </div>
    <script>
      document.addEventListener("DOMContentLoaded", function () {
        var tooltipTriggerList = [].slice.call(
          document.querySelectorAll('[data-bs-toggle="tooltip"]')
        );
        var tooltipList = tooltipTriggerList.map(function (tooltipTriggerEl) {
          return new bootstrap.Tooltip(tooltipTriggerEl);
        });
      });
    </script>
    <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-8152532464536367"
    crossorigin="anonymous"></script>
  </body>
</html>
