                  >
                </div>
              </div>

              <div id="search-results" class="dropdown-menu w-100">
                <!-- Search results here -->
              </div>
            </div>

            <div class="col-12 d-lg-none">
              <!-- Toggle Button -->
              <button
                id="mobileMenuToggle"
                class="btn btn-sm btn-primary mt-3"
                type="button"
                data-bs-toggle="collapse"
                data-bs-target="#mobileMenuItems"
              >
                <i class="bi bi-list me-2"></i> More Options
              </button>

              <!-- Collapsible Menu -->
