    <script>
      (function () {
        function signalGooglefcPresent() {
          if (!window.frames["googlefcPresent"]) {
            if (document.body) {
              const iframe = document.createElement("iframe");
              iframe.style =
                "width: 0; height: 0; border: none; z-index: -1000; left: -1000px; top: -1000px;";
              iframe.style.display = "none";
              iframe.name = "googlefcPresent";
              document.body.appendChild(iframe);
            } else {
              setTimeout(signalGooglefcPresent, 0);
            }
          }
        }
        signalGooglefcPresent();
      })();
    </script>
    <script>
      function shareOnDiscord() {
        const changelogId = localStorage.getItem("selectedChangelogId");
        const title = document.querySelector("#content h1").textContent;
        const url = `${window.location.origin}/changelogs/${changelogId}`;

        // Create a Discord-friendly formatted message
        const discordMessage = `**${title}**\n${url}`;

        // Copy to clipboard
        navigator.clipboard
          .writeText(discordMessage)
          .then(() => {
            toastr.success(
              "Discord formatted message copied to clipboard! Now you can paste it in any Discord channel.",
              "Share",
              {
                positionClass: "toast-bottom-right",
                timeOut: 5000,
                closeButton: true,
                progressBar: true,
                onclick: () => toastr.clear(), // Close on click
              }
            );
          })
          .catch(() => {
            toastr.error("Failed to copy message. Please try again.", "Error", {
              positionClass: "toast-bottom-right",
              timeOut: 3000,
              closeButton: true,
              progressBar: true,
            });
          });
      }

      function shareOnTwitter() {
        const changelogId = localStorage.getItem("selectedChangelogId");
        const title = document.querySelector("#content h1").textContent;
        const url = `${window.location.origin}/changelogs/${changelogId}`;
        const text = `Check out this Roblox Jailbreak changelog: ${title}`;

        const twitterUrl = `https://twitter.com/intent/tweet?text=${encodeURIComponent(
          text
        )}&url=${encodeURIComponent(url)}`;
        window.open(twitterUrl, "_blank");
      }

      function shareOnReddit() {
        const changelogId = localStorage.getItem("selectedChangelogId");
        const title = document.querySelector("#content h1").textContent;
        const url = `${window.location.origin}/changelogs/${changelogId}`;

        const redditUrl = `https://www.reddit.com/submit?url=${encodeURIComponent(
          url
        )}&title=${encodeURIComponent(title)}`;
        window.open(redditUrl, "_blank");
      }

      function copyShareLink() {
        const changelogId = localStorage.getItem("selectedChangelogId");
        const url = `${window.location.origin}/changelogs/${changelogId}`;

        navigator.clipboard.writeText(url).then(() => {
          toastr.success("Link copied to clipboard!", "Share", {
            positionClass: "toast-bottom-right",
            timeOut: 3000,
            closeButton: true,
            progressBar: true,
          });
        });
      }
    </script>
  </body>
</html>
