      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css"
    />
    <link rel="stylesheet" href="styles/faq.css" />
    <link rel="stylesheet" href="styles/navbar.css" />
    <link rel="stylesheet" href="styles/footer.css" />

    <link rel="icon" type="image/svg+xml" href="/icon.svg" />
  </head>
  <body>
    <%- include('partials/nav') %>

    <div class="container content-wrapper">
      <h1 class="text-center mb-4">Frequently Asked Questions</h1>
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">FAQ</li>
        </ol>
      </nav>
      <div id="faq-accordion" class="accordion">
        <!-- FAQ items will be dynamically inserted here -->
      </div>
      <div id="faq-error" class="alert alert-danger" style="display: none;"></div>
    </div>

    <%- include('partials/footer') %>

    <script src="scripts/main.js"></script>
    <script>
      function loadFAQ() {
        const faqAccordion = document.getElementById('faq-accordion');
        const errorDiv = document.getElementById('faq-error');

        fetch('faq.json')
          .then(response => {
            if (!response.ok) {
              throw new Error(`HTTP error! status: ${response.status}`);
            }
            return response.json();
          })
          .then(data => {
            if (!Array.isArray(data) || data.length === 0) {
              throw new Error('FAQ data is empty or not in the expected format');
            }

            data.forEach((item, index) => {
              const accordionItem = document.createElement('div');
              accordionItem.className = 'accordion-item';
              accordionItem.innerHTML = `
                <h2 class="accordion-header">
                  <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse${index}" aria-expanded="false" aria-controls="collapse${index}">
                    ${item.question}
                  </button>
                </h2>
                <div id="collapse${index}" class="accordion-collapse collapse" data-bs-parent="#faq-accordion">
                  <div class="accordion-body">
                    ${item.answer}
                  </div>
                </div>
              `;
              faqAccordion.appendChild(accordionItem);
            });

            console.log('FAQ loaded successfully');
          })
          .catch(error => {
            console.error('Error loading FAQ:', error);
            errorDiv.textContent = `Failed to load FAQ: ${error.message}`;
            errorDiv.style.display = 'block';
          });
      }

      window.addEventListener('load', loadFAQ);
    </script>
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
   <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-8152532464536367"
    crossorigin="anonymous"></script>
  </body>
</html>