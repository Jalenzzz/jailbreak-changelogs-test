<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="referrer" content="no-referrer" />

    <!-- Primary Meta Tags -->
    <title><%= title %></title>
    <meta name="title" content="<%= title %>" />
    <meta
      name="description"
      content="Get detailed information about <%= title.replace(' / Changelogs', '') %> in Jailbreak. View current value, trading history, rarity status, and market trends to make informed trading decisions."
    />
    <meta
      name="keywords"
      content="<%= title.replace(' / Changelogs', '').toLowerCase() %>, Jailbreak <%= itemType.toLowerCase() %>, Jailbreak trading value, <%= itemType.toLowerCase() %> worth, Jailbreak item details, Roblox Jailbreak trading, <%= itemType.toLowerCase() %> rarity, Jailbreak <%= itemType.toLowerCase() %> price Jailbreak <%= itemType.toLowerCase() %> value"
    />
    <meta name="author" content="Jailbreak Changelogs" />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <meta
      property="og:url"
      content="https://testing.jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(itemName) %>"
    />
    <meta
      property="og:title"
      content="<%= title %> - Item Details | JailbreakChangelogs"
    />
    <meta
      property="og:description"
      content="Get detailed information about <%= title.replace(' / Changelogs', '') %> in Jailbreak. View current value, trading history, rarity status, and market trends to make informed trading decisions."
    />
    <meta
      property="og:image"
      content="<%= item?.image || '/assets/logos/Items_Logo.webp' %>"
    />
    <meta property="og:site_name" content="Jailbreak Changelogs Items Page" />

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta
      name="twitter:url"
      content="https://testing.jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(itemName) %>"
    />
    <meta
      name="twitter:title"
      content="<%= title %> - Item Details | JailbreakChangelogs"
    />
    <meta
      name="twitter:description"
      content="Get detailed information about <%= title.replace(' / Changelogs', '') %> in Jailbreak. View current value, trading history, rarity status, and market trends to make informed trading decisions."
    />
    <meta
      name="twitter:image"
      content="<%= item?.image || '/assets/logos/Items_Logo.webp' %>"
    />

    <!-- Additional SEO tags -->
    <meta name="robots" content="index, follow, max-image-preview:large" />
    <link
      rel="canonical"
      href="https://testing.jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(itemName) %>"
    />
    <meta property="og:locale" content="en_US" />
    <meta name="language" content="English" />
    <!-- Google Tag Manager -->
    <script>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({ "gtm.start": new Date().getTime(), event: "gtm.js" });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != "dataLayer" ? "&l=" + l : "";
        j.async = true;
        j.src = "https://www.googletagmanager.com/gtm.js?id=" + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, "script", "dataLayer", "GTM-KM853GJ6");
    </script>
    <!-- End Google Tag Manager -->

    <!-- Schema.org markup for Google -->
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "Product",
        "name": "<%= title.replace(' / Changelogs', '') %>",
        "image": "<%= item?.image || '/assets/logos/Items_Logo.webp' %>",
        "description": "Get detailed information about <%= title.replace(' / Changelogs', '') %> in Jailbreak. View current value, trading history, rarity status, and market trends.",
        "category": "<%= itemType %>",
        "url": "https://testing.jailbreakchangelogs.xyz/item/<%= itemType %>/<%= encodeURIComponent(itemName) %>"
      }
    </script>

    <base href="/" />

    <link rel="icon" type="image/svg+xml" href="/icon.svg" />

    <!-- Stylesheets -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css"
    />

    <link rel="stylesheet" href="styles/item.css" />
    <link rel="stylesheet" href="styles/main.css" />

    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.7/dist/chart.umd.min.js"></script>
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/icon.svg" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Luckiest+Guy&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.css"
      integrity="sha512-6S2HWzVFxruDlZxI3sXOZZ4/eJ8AcxkQH1+JjSe/ONCEqR9L4Ysq5JdT5ipqtzU7WHalNwzwBv+iE51gNHJNqQ=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
  </head>

  <body data-scroll-container>
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-KM853GJ6"
        height="0"
        width="0"
        style="display: none; visibility: hidden"
      ></iframe
    ></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!-- prettier-ignore -->
    <%- include('partials/nav') %>
    <%- include('partials/announcement') %>

    <div id="item-container">
      <!-- Item will be loaded here dynamically -->
      <div class="d-flex justify-content-center mt-5">
        <div class="custom-spinner" role="status">
          <span class="visually-hidden">Loading...</span>
        </div>
      </div>
    </div>

    <div class="comment-container my-5 container-fluid px-3 px-sm-auto">
      <div class="row justify-content-center">
        <div class="col-12 col-md-10 col-lg-8">
          <!-- Comment List Container -->
          <div
            class="comments-wrapper p-4"
            style="
              background-color: #212a31;
              border-radius: 8px;
              border: 2px solid #748d92;
            "
          >
            <!-- Comments will be loaded dynamically -->
          </div>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>

    <!-- Scripts -->
    <script
      src="https://code.jquery.com/jquery-3.7.1.min.js"
      integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo="
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js"
      integrity="sha512-lbwH47l/tPXJYG9AcFNoJaTMhGvYWhVM9YI43CT+uteTRRaiLCui8snIgyAN8XWgNjNhCqlAUdzZptso6OCoFQ=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script src="scripts/main.js"></script>
    <script src="scripts/item.js" defer></script>
  </body>
</html>
