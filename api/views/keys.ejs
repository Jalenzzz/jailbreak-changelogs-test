      {
        "@context": "https://schema.org",
        "@type": "WebApplication",
        "name": "JailbreakChangelogs API Management",
        "applicationCategory": "DeveloperApplication",
        "operatingSystem": "Any",
        "description": "API key management system for accessing Jailbreak game data and features",
        "url": "https://testing.jailbreakchangelogs.xyz/keys",
        "accessMode": "textual",
        "applicationSubCategory": "API Management Tool",
        "availableOnDevice": "Web browser",
        "permissions": [
          "Changelogs access",
          "Season data access",
          "User data access",
          "Items data access",
          "Comments access"
        ]
      }
    </script>

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />

    <link rel="stylesheet" href="styles/keys.css" />
    <link rel="stylesheet" href="styles/main.css" />

    <link rel="icon" type="image/svg+xml" href="/icon.svg" />

    <link
      href="https://fonts.googleapis.com/css2?family=Luckiest+Guy&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.css"
      integrity="sha512-6S2HWzVFxruDlZxI3sXOZZ4/eJ8AcxkQH1+JjSe/ONCEqR9L4Ysq5JdT5ipqtzU7WHalNwzwBv+iE51gNHJNqQ=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css"
    />
  </head>
  <body>
    <!-- prettier-ignore -->
    <%- include('partials/nav') %>
    <%- include('partials/newsticker') %>

    <div class="container content-wrapper">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">Api Keys</li>
        </ol>
      </nav>
      <h1>API Key Management</h1>
      <button
        class="btn btn-primary mb-3"
        data-bs-toggle="modal"
        data-bs-target="#createKeyModal"
      >
        Create API Key
      </button>
      <div class="alert alert-info mb-4" role="alert">
        Documentation can be found
        <a href="https://docs.jailbreakchangelogs.xyz">here.</a>
      </div>
      <div class="table-responsive">
        <table
          class="table table-dark table-bordered table-hover table-striped"
        >
          <thead>
            <tr>
              <th>Manage</th>
              <th>Key</th>
              <th>Name</th>
              <th>Description</th>
              <th>Permissions</th>
              <th>Created At</th>
            </tr>
          </thead>
          <tbody id="apiKeyList">
            <!-- API Keys will be appended here -->
          </tbody>
        </table>
      </div>
    </div>

    <!-- Create API Key Modal -->
    <div
      class="modal fade"
      id="createKeyModal"
      tabindex="-1"
      aria-labelledby="createKeyModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog">
        <div
          class="modal-content"
          style="background-color: rgba(33, 37, 41, 0.9)"
        >
          <div class="modal-header">
            <h5 class="modal-title" id="createKeyModalLabel">Create API Key</h5>
            <button
              type="button"
              class="btn-close"
              data-bs-dismiss="modal"
              aria-label="Close"
            ></button>
          </div>
          <div class="modal-body">
            <div class="mb-3">
              <label for="keyName" class="form-label">Name</label>
              <input
                type="text"
                class="form-control"
                id="keyName"
                placeholder="Enter key name"
              />
            </div>
            <div class="mb-3">
              <label for="keyDescription" class="form-label">Description</label>
              <input
                type="text"
                class="form-control"
                id="keyDescription"
                placeholder="Enter key description"
              />
            </div>
            <div class="mb-3">
              <label class="form-label">Permissions</label><br />
              <div class="form-check">
                <input
                  class="form-check-input"
                  type="checkbox"
                  id="permChangelog"
                  value="changelog"
                />
                <label class="form-check-label" for="permChangelog"
                  >Changelogs</label
                >
              </div>
              <div class="form-check">
                <input
                  class="form-check-input"
                  type="checkbox"
                  id="permSeasons"
                  value="seasons"
                />
                <label class="form-check-label" for="permUserManagement"
                  >Seasons</label
                >
              </div>
              <div class="form-check">
                <input
                  class="form-check-input"
                  type="checkbox"
                  id="permComments"
                  value="comments"
                />
                <label class="form-check-label" for="permAdmin">Comments</label>
              </div>
              <div class="form-check">
                <input
                  class="form-check-input"
                  type="checkbox"
                  id="permUsers"
                  value="users"
                />
                <label class="form-check-label" for="permUsers">Users</label>
              </div>
              <div class="form-check">
                <input
                  class="form-check-input"
                  type="checkbox"
                  id="permItems"
                  value="items"
                />
                <label class="form-check-label" for="permItems">Items</label>
              </div>
              <!-- Add more checkboxes as needed -->
            </div>
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-secondary"
              data-bs-dismiss="modal"
            >
              Close
            </button>
            <button type="button" class="btn btn-primary" id="createKeyButton">
              Create Key
            </button>
          </div>
        </div>
      </div>
    </div>

    <%- include('partials/footer') %>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script src="scripts/api.js"></script>
    <script src="scripts/main.js"></script>
